o,\u000a                'z-index': '2147483647'\u000a            })\u000a        }\u000a        $(window).scroll(function() {\u000a            if (an === "fade") {\u000a                $(($(window).scrollTop() > td) ? $(sn).fadeIn(is) : $(sn).fadeOut(os));\u000a            } else if (an === "slide") {\u000a                $(($(window).scrollTop() > td) ? $(sn).slideDown(is) : $(sn).slideUp(os));\u000a            } else {\u000a                $(($(window).scrollTop() > td) ? $(sn).show(0) : $(sn).hide(0));\u000a            }\u000a        });\u000a        $(sn).click(function(event) {\u000a            $('html, body').animate({\u000a                scrollTop: 0\u000a            }, ts);\u000a            return false;\u000a        });\u000a    };\u000a}(jQuery));\u000a$.scrollUp();\u000a\u000a// \u6dfb\u52a0\u51fa\u5904\u53ca\u5feb\u6377\u70b9\u51fb\u63a8\u8350\u000a$(function(){\u000a    var body=$("#cnblogs_post_body");\u000a    body.append("<div style='color: #1a8bc8;font-weight: bold;font-size: 16px;'><br/>\u5982\u679c\u60a8\u89c9\u5f97\u8fd9\u7bc7\u6587\u7ae0\u5bf9\u60a8\u6709\u5e2e\u52a9\uff0c\u6b22\u8fce\u70b9\u51fb\u53f3\u4e0b\u89d2<span style='color: red'>\u3010</span><span id='mydiv_digg'>\u63a8\u8350</span><span  style='color: red'>\u3011</span>\u652f\u6301\u4e00\u4e0b\u6211\uff0c\u8c22\u8c22\uff01</div>");\u000a    $("#mydiv_digg").on("click", function() {\u000a        $("#div_digg .diggit").click();\u000a    });\u000a    //\u000a    if(!/^\u005c(\u8f6c\u005c)|\u3010\u8f6c\u3011/.test($("#cb_post_title_url").text())){\u000a        body.append("<div style='color:red;font-weight: bold;font-size: 16px;'><br/>\u8fd9\u7bc7\u6587\u7ae0\u4e3a\u539f\u521b\u6587\u7ae0\uff0c\u8f6c\u8f7d\u8bf7\u6ce8\u660e\u51fa\u5904\uff01<br/></div>");\u000a        body.append("<div style='font-size: 16px;'>\u4f5c\u8005 TZYY / \u539f\u6587\u94fe\u63a5 <a  style='border: none;color:#1a8bc8;' href='"+location.href+"'>"+location.href+"</a></div>");\u000a 