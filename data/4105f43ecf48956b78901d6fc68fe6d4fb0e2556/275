umpy' href='http://blog.pluskid.org/?p=71' />\u000a<link rel='canonical' href='http://blog.pluskid.org/?p=39' />\u000a<link rel='shortlink' href='http://blog.pluskid.org/?p=39' />\u000a\u000a<script language="JavaScript" type="text/javascript"><!--\u000a\u000afunction expand(param) {\u000a jQuery("div"+param).stop().fadeToggle(400, function() {\u000a    linkname = jQuery("a"+param).html();\u000a    if( jQuery("div"+param).is(":visible") ) {\u000a      jQuery("a"+param).html(expand_text+" "+linkname.substring(collapse_text_length));\u000a    }\u000a    else {\u000a        jQuery("a"+param).html(collapse_text+" "+linkname.substring(expand_text_length));\u000a    }\u000a  });\u000a\u000a}\u000afunction expander_hide(param) {\u000a        jQuery("div"+param).hide();\u000a		linkname = jQuery("a"+param).html();\u000a        collapse_text  = "\u21d3 \u70b9\u51fb\u5c55\u5f00";\u000a        expand_text    = "\u21d1 \u70b9\u51fb\u6298\u53e0";\u000a        collapse_text_length = jQuery("<span />").html(collapse_text).text().length;\u000a        expand_text_length = jQuery("<span />").html(collapse_text).text().length;\u000a        jQuery("a"+param).html(collapse_text + " " + linkname);\u000a        jQuery("a"+param).show();\u000a}\u000a//--></script>\u000a\u000a<!-- Protected by WP-SpamFree :: JS BEGIN -->\u000a<script type="text/javascript" src="http://blog.pluskid.org/wp-content/plugins/wp-spamfree/js/wpsfv2-js.php"></script> \u000a<!-- Protected by WP-SpamFree :: JS END -->\u000a\u000a<style type="text/css">body{text-align:center;margin:0;padding:0;font-family:tahoma,arial,sans-serif;font-size:0.9em;line-height:20px;color:#000000;background:#ffffff}a:link,a:visited,a:active{color:#2A6387;font-weight:normal;text-decoration:none;}a:hover{color:#CC0000;font-weight:normal;text-decoration:none}ul,ol,dl,p,h1,h2,h3,h4,h5,h6{margin-top:10px;margin-bottom:10px;padding-top:0;padding-bottom:0;}ul ul,ul ol,ol ul,ol ol{margin-top:0;margin-bottom:0}h1,h2,h3,h4,h