pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/sale-newgoods/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/sale/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/ltd-offer-newgoods/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/ltd-offer/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/set-newgoods/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					 pos=referrer.indexOf("/set/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a							}\u000a					\u000a					\u000a					pos=referrer.indexOf("/feature/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/contents/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					pos=referrer.indexOf("/store/category/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a							startpos = pos;\u000a						}\u000a					\u000a					pos=referrer.indexOf("/goods/");\u000a					if(pos>0 && (startpos==0 || pos < startpos))\u000a						{\u000a						return "L4";\u000a						}\u000a\u000a					if(null != startpos && startpos>0)\u000a						{\u000a							url=referrer.toString().substring(startpos);\u000a							return top3(url);\u000a						}\u000a\u000a					pos=referrer.indexOf("/FSC02210E01.do");\u000a					if(pos>0)\u000a						{\u000a						return  "ProductSearch";\u000a						}\u000a\u000a					pos=referrer.indexOf("/FSC03010");\u000a					if(pos>0)\u000a						{\u000a						return "Cart";\u000a						}\u000a\u000a					pos=referrer.indexOf("/FSC03070SCR.do");\u000a					if(pos>0)\u000a						{\u000a						return "Affiliate";\u000a						}\u000a\u000a					return "ShopCNDomain";\u000a\u000a			}\u000a			else\u000a			{\u000a				pos=referrer.indexOf(topDomain);\u000a					if(pos>=0)\u000a					{\u000a						return "wwwDomain";\u000a					}\u000a					else\u000a					{\u000a						return "OtherDomain";\u000a					}\u000a			}\u000a\u000a		}\u000a	}\u000a\u000a\u000afunction top3(url)\u000a{\u000a	var urlTokens = url.split( "/" ); // get tokens\u000a  	var returnVal= "";\u000a	var noOfTokens = 0;\u000a	var pos = 0;\u000a\u000a	noOfTokens = urlTokens.length - 1; //it gives the exact no. of tokens\u000a\u000a	if(4 < noOfTokens){\u000a		noOfTokens = 4; 								// if no. of tokens are more than 4, take first 4 tokens only starting with /\u000a	}\u000a\u000a\u000a	for ( var i = 1; i <= noOfTokens; i++ )	{\u000a    	returnVal= returnVal+ "/" + urlTokens[i] ;\u000a    }\u000a\u000a	pos = returnVal.lastIndexOf("/");\u000a\u000a	if(((pos+1) == returnVal.length)){\u000a	returnVal = returnVal.substring(0,(returnVal.length) - 1);\u000a	}\u000a	return returnVal;\u000a}\u000a</script>\u000a\u000a\u000a\u000a\u000a\u000a\u000a    \u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a	<!-- FROM footer.html -->\u000a	<!-- begin: footer section -->\u000a    \u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a\u000a<footer id="footerWrap">\u000a<div id="footer">\u000a<!-- start FSC-footer_01_CN // -->\u000a	<nav id="snCategory">\u000a		<h2 class="headClose">\u5546\u54c1\u5206\u7c7b</h2>\u000a		<ul style="display: none;">\u000a			<li class="D00010"><h3 class="contentClose">\u6d3b\u52a8\u7279\u96c6</h3>\u000a				<